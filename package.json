{
  "name": "ro-sa",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --aot",
    "build": "ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.ts",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.ts --app 'yarn start' --app-init-delay 4000",
    "lint": "ng lint --format codeFrame",
    "format": "prettier {src,e2e}/**/*.{ts,tsx,json,md,scss,css,less} --write",
    "style": "yarn format && yarn lint --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.5",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/router": "^5.2.5",
    "core-js": "2.5.3",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.8",
    "@angular/compiler-cli": "5.2.5",
    "@angular/language-service": "5.2.5",
    "@types/jest": "22.1.2",
    "@types/node": "~6.0.60",
    "codelyzer": "4.1.0",
    "jest": "22.2.2",
    "jest-preset-angular": "5.0.0",
    "prettier": "1.10.2",
    "testcafe": "0.18.6",
    "testcafe-angular-selectors": "0.3.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.8.0",
    "typescript": "2.6.2"
  }
}
